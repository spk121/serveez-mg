#ifndef __SVZCONFIG_H__
#define __SVZCONFIG_H__ 1

/* Define to 'int' if <winsock2.h> does not define 'HANDLE'. */
#undef svz_t_handle

/* Define to 'int' if <winsock2.h> does not define 'SOCKET'. */
#undef svz_t_socket

/* Define the signal()'s detected return type. */
#undef svz_t_retsig

/* Define to empty if the keyword does not work.  */
#undef svz_c_const

/* Define to empty if the keyword does not work. */
#undef svz_c_signed

/* Define to `unsigned' if <sys/types.h> doesn't define 'size_t'. */
#undef svz_t_size

/* Define to `long' if <sys/types.h> doesn't define 'off_t'. */
#undef svz_t_off

/* Define if debug output should be supported. */
#undef SVZ_ENABLE_DEBUG

/* Define if flood protection should be supported. */
#undef SVZ_ENABLE_FLOOD_PROTECTION

/* Define if you have the <varargs.h> header file.  */
#undef SVZ_HAVE_VARARGS_H

/* Define if you have the <pthread.h> header file.  */
#undef SVZ_HAVE_PTHREAD_H

/* Define if you have the snprintf function. */
#undef SVZ_HAVE_SNPRINTF

/* Define if you have the vsnprintf function. */
#undef SVZ_HAVE_VSNPRINTF

/* Define if you have a thread library. */
#undef SVZ_HAVE_THREADS

/* Define if you have the crypt function. */
#undef SVZ_HAVE_CRYPT

#endif /* !__SVZCONFIG_H__ */
